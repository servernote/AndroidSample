<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="-2dp"
        android:left="0dp"
        android:right="-2dp"
        android:top="-2dp">

        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#555555"
                android:dashWidth="2dp"
                android:dashGap="2dp" />
        </shape>
    </item>
</layer-list>
